<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Most Popular</h3>
    </div>
    <div class="panel-body">
        {{#each sidebar.popular}}
            <div style="padding-bottom: .5em;" class="col-md-4 text-center">
                <a href="/images/{{uniqueId}}"><img src="/public/upload/{{filename}}" style="width: 75px; height: 75px;" alt="" class="img-thumbnail"></a>
            </div>
        {{/each}}
    </div>
</div>